	void CArtiGlobalTest::ArtiGlobalTest_RpcSendRecv()
	{
		CAlgorithmData *data2 = new CAlgorithmData();

		CArtiMenu	uiMenu;

		uiMenu.InitTitle("RpcSendRecv");

		uiMenu.AddItem("code 0 or other");
		uiMenu.AddItem("code -1");
		while (1)
		{
			uint32_t uRet = uiMenu.Show();
			if (uRet==DF_ID_BACK)
			{
				break;
			}
			uRet &= 0xff;
			if (uRet==0)
			{
				data2->SetId(0x0206);
				data2->SetSeed(CBinary("\x12\x34\x56\x78", 4));
				data2->SetSeedType(CAlgorithmData::SEED_TYPE_LOCAL);

				uint32_t ret = CArtiGlobal::RpcSendRecv(data2);
				if (ret == -1)
				{
					ShowMsgBoxDemo(artiGetText("FF040000001A"), artiGetText("FF040000001C"), DF_MB_OK, DT_LEFT, -1, m_uThread);
				}
				else
				{
					string data_len = to_string(ret);
					ShowMsgBoxDemo(artiGetText("FF040000001A"), artiGetText("FF040000001B") + data_len, DF_MB_OK, DT_LEFT, -1, m_uThread);
				}
			}
			else
			{
				uint32_t ret = CArtiGlobal::RpcSendRecv(nullptr);
				string data_len = to_string(ret);
				ShowMsgBoxDemo(artiGetText("FF040000001A"), artiGetText("FF040000001B") + data_len, DF_MB_OK, DT_LEFT, -1, m_uThread);
			}
		}

	}

