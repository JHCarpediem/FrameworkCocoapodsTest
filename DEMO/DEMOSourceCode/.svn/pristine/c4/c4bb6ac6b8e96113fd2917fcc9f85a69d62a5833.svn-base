	void CArtiReportTest::ArtiReportTest_InitTitle()
	{
		// 1.配置接口参数初始值
		map<string, string> mapTitle;
		mapTitle.emplace(artiGetText("FF00000000E0"), artiGetText("FF00000000E0"));	//"诊断报告"
		mapTitle.emplace(artiGetText("FF000000010B"), artiGetText("FF000000010B"));	//"Diagnosis report"

		string strTitle = artiGetText("FF00000000E0");								//诊断报告

		// 2.显示参数列表
		CArtiList uiList;
		uiList.InitTitle("InitTitle");
		uiList.SetColWidth(m_vctColWidth2);
		uiList.SetHeads(m_vctHeads);
		uiList.AddItem("strTitle");
		uiList.SetItem(0, 1, strTitle);
		uiList.AddButtonEx("Test");
		//uiList.AddButtonEx("Help");
		
		// 3.配置和测试接口
		uint32_t uRetBtn = DF_ID_NOKEY;
		while (true)
		{
			Delay(100);
			uRetBtn = uiList.Show();
			if (DF_ID_BACK == uRetBtn)
			{
				break;
			}
			else if (DF_ID_FREEBTN_0 == uRetBtn)
			{
				//设置系统报告
				CArtiReport uiReport;

				// 测试接口
				uiReport.InitTitle(strTitle);

				// 固定设置
				uiReport.SetReportType(CArtiReport::REPORT_TYPE_SYSTEM);
				uiReport.SetTypeTitle(artiGetText("FF0000000101"));																	//系统诊断报告
				uiReport.SetDescribeTitle(artiGetText("FF0000000100"));																//2015-09奥迪
				uiReport.SetVehInfo(artiGetText("FF00000000F2"), artiGetText("FF00000000F3"), artiGetText("FF00000000F4"));			//宝马
				uiReport.SetEngineInfo(artiGetText("FF00000000F6"), artiGetText("FF00000000F7"));
				uiReport.SetVehPath(artiGetText("FF00000000FC"));																	//宝马>320>系统>自动扫描
				uiReport.SetSummarize(artiGetText("FF0000000107"), artiGetText("FF0000000108"));									//"总共扫描出12个系统，其中12个系统故障，故障数量为105个。为了安全驾驶请你仔细阅读分析报告，并修复相关故障信息组件，及时处理排查。"
				uiReport.AddSysItems(vctSysReportItem);
				uiReport.AddDtcItems(vctDtcReportItem);
				uiReport.AddLiveDataItems(vctDsReportItem);

				while (1)
				{
					uRetBtn = uiReport.Show();
					if (uRetBtn == DF_ID_BACK)
					{
						break;
					}
				}
			}
			else if (DF_ID_FREEBTN_1 == uRetBtn)
			{

			}
			else if (DF_ID_NOKEY != uRetBtn)
			{
				uint16_t uSelect = uiList.GetSelectedRow();
				if (0 == uSelect)
				{
					GetParamValue(strTitle, strTitle, mapTitle);
					uiList.SetItem(0, 1, strTitle);
				}
			}
		}
	}

