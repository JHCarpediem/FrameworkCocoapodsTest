	CArtiReportTest::CArtiReportTest()
	{
		m_vctColWidth2.clear();
		m_vctColWidth2.push_back(40);
		m_vctColWidth2.push_back(60);

		m_vctHeads.clear();
		m_vctHeads.push_back(artiGetText("FF0000000051"));	//参数
		m_vctHeads.push_back(artiGetText("FF000000002F"));	//参数值

		// 设置系统信息
		vctSysReportItem.clear();
		for (uint8_t i = 1; i < 6; i++)
		{
			stSysReportItem sysReportItem;
			sysReportItem.strID = uint32ToString(i);

			CBinary binSysName = CBinary("\x50\x00\x00\x05\x00\x00", 6);
			binSysName.SetAt(5, i);
			sysReportItem.strName = artiGetText(binSysName);

			sysReportItem.uDtsNums = 2;
			vctSysReportItem.push_back(sysReportItem);
		}

		vctSysReportItem24.clear();
		for (uint8_t i = 1; i < vctSysReportItem.size(); i = i + 2)
		{
			vctSysReportItem24.push_back(vctSysReportItem[i]);
		}

		vctSysReportItem135.clear();
		for (uint8_t i = 0; i < vctSysReportItem.size(); i = i + 2)
		{
			vctSysReportItem135.push_back(vctSysReportItem[i]);
		}

		// 设置故障码信息
		vctDtcReportItem.clear();
		m_vctDtcReportItemEx.clear();

		for (uint8_t i = 1; i < 6; i++)
		{
			stDtcReportItem DtcReportItem;
			stDtcReportItemEx DtcReportItemEx;

			DtcReportItem.strID = uint32ToString(i);
			DtcReportItemEx.strID = uint32ToString(i);

			CBinary binSysName = CBinary("\x50\x00\x00\x05\x00\x00", 6);
			binSysName.SetAt(5, i);
			DtcReportItem.strName = artiGetText(binSysName);
			DtcReportItemEx.strName = artiGetText(binSysName);

			stDtcNode dtcNode;
			dtcNode.strCode = "PC0015";
			dtcNode.strDescription = artiGetText("FF0000000109");//位置传感器短路
			dtcNode.uStatus = DF_DTC_STATUS_CURRENT;
			DtcReportItem.vctNode.push_back(dtcNode);

			stDtcNode dtcNode1;
			dtcNode1.strCode = "PC0016";
			dtcNode1.strDescription = artiGetText("FF0000000109");//位置传感器短路
			dtcNode1.uStatus = DF_DTC_STATUS_HISTORY;
			DtcReportItem.vctNode.push_back(dtcNode1);
			vctDtcReportItem.push_back(DtcReportItem);


			stDtcNodeEx dtcNodeEx;
			dtcNodeEx.strCode = "PC0017";
			dtcNodeEx.strDescription = artiGetText("FF0000000109");//位置传感器短路
			dtcNodeEx.strStatus = artiGetText("FF0C00000013");//位置传感器短路;
			dtcNodeEx.uStatus = DF_DTC_STATUS_CURRENT;
			DtcReportItemEx.vctNode.push_back(dtcNodeEx);

			stDtcNodeEx dtcNodeEx1;
			dtcNodeEx1.strCode = "PC0018";
			dtcNodeEx1.strDescription = artiGetText("FF0000000109");//位置传感器短路
			dtcNodeEx1.strStatus = artiGetText("FF0C00000014");//位置传感器短路;
			dtcNodeEx1.uStatus = DF_DTC_STATUS_HISTORY;
			DtcReportItemEx.vctNode.push_back(dtcNodeEx);
			
			m_vctDtcReportItemEx.push_back(DtcReportItemEx);
		}

		vctDtcReportItem24.clear();
		for (uint8_t i = 1; i < vctDtcReportItem.size(); i = i+2)
		{
			vctDtcReportItem24.push_back(vctDtcReportItem[i]);
		}

		vctDtcReportItem135.clear();
		for (uint8_t i = 0; i < vctDtcReportItem.size(); i = i + 2)
		{
			vctDtcReportItem135.push_back(vctDtcReportItem[i]);
		}

		// 设置数据流信息
		vctDsReportItem.clear();

		CBinary binName = CBinary("\x50\x00\x00\x08\x00\x00", 6);
		CBinary binUnit = CBinary("\x50\x00\x00\x09\x00\x00", 6);

		for (uint8_t i = 1; i < 6; i++)
		{
			binName.SetAt(5, i);
			binUnit.SetAt(5, i);
			
			stDsReportItem dsReportItem;
			dsReportItem.strName = artiGetText(binName);
			dsReportItem.strValue = uint32ToString(i * 50);
			dsReportItem.strUnit = artiGetText(binUnit);
			dsReportItem.strMin = "0";
			dsReportItem.strMax = "100";
			dsReportItem.strReference = "50";
			vctDsReportItem.push_back(dsReportItem);
		}

		vctDsReportItem24.clear();
		for (uint8_t i = 1; i < vctDsReportItem.size(); i = i + 2)
		{
			vctDsReportItem24.push_back(vctDsReportItem[i]);
		}

		vctDsReportItem135.clear();
		for (uint8_t i = 0; i < vctDsReportItem.size(); i = i + 2)
		{
			vctDsReportItem135.push_back(vctDsReportItem[i]);
		}
	}

	void CArtiReportTest::ShowMenu()
	{
		vector<string> vctMenuName;
		vctMenuName.push_back("InitTitle");
		vctMenuName.push_back("SetReportType");
		vctMenuName.push_back("SetTypeTitle");
		vctMenuName.push_back("SetDescribeTitle");
		vctMenuName.push_back("SetSummarize");
		vctMenuName.push_back("AddSysItem");
		vctMenuName.push_back("AddSysItems");
		vctMenuName.push_back("AddDtcItem");
		vctMenuName.push_back("AddDtcItems");
		vctMenuName.push_back("AddLiveDataSys");
		vctMenuName.push_back("AddLiveDataItem");
		vctMenuName.push_back("AddLiveDataItems");
		vctMenuName.push_back("SetMileage");
		vctMenuName.push_back("SetVehInfo");
		vctMenuName.push_back("SetEngineInfo");
		vctMenuName.push_back("SetVehPath");
		vctMenuName.push_back("AddDtcItemEx");
		vctMenuName.push_back("AddDtcItemsEx");

		CArtiMenu uiMenu;
		uiMenu.InitTitle(artiGetText("FF0000000001"));//接口列表
		for (uint8_t i = 0; i < vctMenuName.size(); i++)
		{
			uiMenu.AddItem(vctMenuName[i]);
		}

		uint32_t uRetBtn = DF_ID_NOKEY;

		while (1)
		{
			uRetBtn = uiMenu.Show();
			if (DF_ID_BACK == uRetBtn)
			{
				break;
			}
			else if (uRetBtn < vctMenuName.size())
			{
				if ("InitTitle" == vctMenuName[uRetBtn])
				{
					ArtiReportTest_InitTitle();
				}
				else if ("SetReportType" == vctMenuName[uRetBtn])
				{
					ArtiReportTest_SetReportType();
				}
				else if ("SetTypeTitle" == vctMenuName[uRetBtn])
				{
					ArtiReportTest_SetTypeTitle();
				}
				else if ("SetDescribeTitle" == vctMenuName[uRetBtn])
				{
					ArtiReportTest_SetDescribeTitle();
				}
				else if ("SetSummarize" == vctMenuName[uRetBtn])
				{
					ArtiReportTest_SetSummarize();
				}
				else if ("AddSysItem" == vctMenuName[uRetBtn])
				{
					ArtiReportTest_AddSysItem();
				}
				else if ("AddSysItems" == vctMenuName[uRetBtn])
				{
					ArtiReportTest_AddSysItems();
				}
				else if ("AddDtcItem" == vctMenuName[uRetBtn])
				{
					ArtiReportTest_AddDtcItem();
				}
				else if ("AddDtcItems" == vctMenuName[uRetBtn])
				{
					ArtiReportTest_AddDtcItems();
				}
				else if ("AddLiveDataSys" == vctMenuName[uRetBtn])
				{
					ArtiReportTest_AddLiveDataSys();
				}
				else if ("AddLiveDataItem" == vctMenuName[uRetBtn])
				{
					ArtiReportTest_AddLiveDataItem();
				}
				else if ("AddLiveDataItems" == vctMenuName[uRetBtn])
				{
					ArtiReportTest_AddLiveDataItems();
				}
				else if ("SetMileage" == vctMenuName[uRetBtn])
				{
					ArtiReportTest_SetMileage();
				}
				else if ("SetVehInfo" == vctMenuName[uRetBtn])
				{
					ArtiReportTest_SetVehInfo();
				}
				else if ("SetEngineInfo" == vctMenuName[uRetBtn])
				{
					ArtiReportTest_SetEngineInfo();
				}
				else if ("SetVehPath" == vctMenuName[uRetBtn])
				{
