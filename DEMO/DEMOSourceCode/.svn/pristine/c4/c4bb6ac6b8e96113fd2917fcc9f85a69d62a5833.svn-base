	void CArtiReportTest::ArtiReportTest_SetSummarize()
	{
		// 1.配置接口参数初始值
		map<string, string> mapTitle;
		map<string, string> mapContent;

		mapTitle.emplace(artiGetText("FF0000000107"), artiGetText("FF0000000107"));//"概述"
		mapTitle.emplace(artiGetText("FF000000010D"), artiGetText("FF000000010D"));//"Summarize"

		mapContent.emplace(artiGetText("FF0000000108"), artiGetText("FF0000000108"));
		mapContent.emplace(artiGetText("FF000000010E"), artiGetText("FF000000010E"));

		string strTitle = artiGetText("FF0000000107");		//"概述"
		string strContent = artiGetText("FF0000000108");	//"总共扫描出12个系统，其中12个系统故障，故障数量为105个。为了安全驾驶请你仔细阅读分析报告，并修复相关故障信息组件，及时处理排查。"

		// 2.显示参数列表
		CArtiList uiList;
		uiList.InitTitle("SetSummarize");
		uiList.SetColWidth(m_vctColWidth2);
		uiList.SetHeads(m_vctHeads);
		uiList.AddItem("strTitle");
		uiList.AddItem("strContent");
		uiList.SetItem(0, 1, strTitle);
