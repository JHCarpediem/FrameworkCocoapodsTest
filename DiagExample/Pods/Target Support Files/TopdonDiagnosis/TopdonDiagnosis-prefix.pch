#define NSLog(...) NSLog(__VA_ARGS__);

//#define BLELog(...);
//
//#define HLog(...);

#define BLELog(fmt, ...) ({\
@autoreleasepool {\
if ([TDD_DiagnosisManage sharedManage].enabledBleLog) {\
[TDD_Tools bleLog:[NSString stringWithFormat:(@"%@",@"" fmt), ##__VA_ARGS__] file:[[NSString stringWithUTF8String:__FILE__] lastPathComponent] func:[NSString stringWithFormat:@"%s", __PRETTY_FUNCTION__] line:__LINE__];\
}\
}\
});

#define HLog(fmt, ...) ({\
@autoreleasepool {\
if ([TDD_DiagnosisManage sharedManage].enabledAD200Log) {\
[TDD_Tools diagLog:[NSString stringWithFormat:(@"%@",@"" fmt), ##__VA_ARGS__] file:[[NSString stringWithUTF8String:__FILE__] lastPathComponent] func:[NSString stringWithFormat:@"%s", __PRETTY_FUNCTION__] line:__LINE__];\
}\
}\
});

